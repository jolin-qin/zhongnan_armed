page {
	background: url('https://zntest.wxumi.com/armed/bg.jpg') no-repeat center top;
    background-size: cover;
    overflow-x: hidden;
}
.container {
	padding-top: 1px;
}
.rank_btn {
	display: block;
	width: 198rpx;
	height: 66rpx;
	margin: 72rpx 0 0 532rpx;
}
.main_title01 {
	display: block;
	width: 352rpx;
	height: 40rpx;
	margin: 60rpx auto 0;
}
.main_title02 {
	display: block;
	width: 582rpx;
	height: 78rpx;
	margin: 40rpx auto 0;
}
.title_pic {
	display: block;
	width: 410rpx;
	height: 200rpx;
	margin: 166rpx auto 66rpx;
}
.start_wrapper {
	width: 616rpx;
	height: 710rpx;
	background: url('https://zntest.wxumi.com/armed/sy_bg.png') no-repeat center;
	background-size: 616rpx 710rpx;
	margin: 6rpx auto 0;
	position: relative;
	padding-top: 196rpx;
	color: #88e5d9;
}
.head_portrait {
	width: 136rpx;
	height: 136rpx;
	border-radius: 50%;
	overflow: hidden;
	position: absolute;
	background-color: #fff;
	top: 14rpx;
	left: 50%;
	transform: translate(-55%);
}
.information_item {
	width: 544rpx;
	height: 70rpx;
	line-height: 66rpx;
	background-color: #000000;
	border: solid 2rpx #169b8a;
	padding-left: 20rpx;
	margin: 32rpx auto 0;
}
.have_got_img {
	display: block;
	width: 506rpx;
	height: 122rpx;
	margin: 24rpx auto 0;
}
.condition {
	color: #95f6e9;
	text-align: center;
	font-size: 24rpx;
	margin-top: 18rpx;
}
.condition .txt {
	color: #fffc00;
}
.btn_wrapper {
	position: absolute;
	bottom: -144rpx;
	left: 50%;
	width: 692rpx;
	height: 110rpx;
	transform: translateX(-50%);

}
.btn_wrapper .btn_item {
	display: block;
	width: 210rpx;
	height: 110rpx;
}
/* 确认弹窗css */
.confirm_popup .van-popup {
	background-color: transparent !important;
}
.confirm_popup_wrapper {
	width: 640rpx;
	height: 550rpx;
	background-color: #012F2F;
	border: 1px solid #95f6e9;
	color: #95f6e9;
}
.confirm_popup_wrapper .img {
	display: block;
	margin: 50rpx auto;
}
.confirm_popup_wrapper .chuji {
	width: 192rpx;
	height: 192rpx;
}
.confirm_popup_wrapper .gaoji {
	width: 236rpx;
	height: 236rpx;
	margin: 24rpx auto 0;
}
.confirm_popup_wrapper .yidong {
	width: 322rpx;
	height: 214rpx;
	margin: 30rpx auto 0;
}
.confirm_popup_wrapper .confirm_txt {
	text-align: center;
	margin: 40rpx 0 50rpx;
}
.confirm_popup_wrapper .blue {
	color: #04B9F5;
}
.confirm_popup_wrapper .yellow {
	color: #fffc00;
}
.confirm_popup_wrapper .move {
	color: red;
}
.confirm_popup_wrapper .confirm_btn {
	width: 362rpx;
	height: 130rpx;
	line-height: 130rpx;
	margin: 0 auto;
	background: url('https://zntest.wxumi.com/armed/tongyong_btn_h.png') no-repeat center;
	background-size: 362rpx 130rpx;
	text-align: center;
	font-size: 56rpx;
	font-weight: bold;
	color: #fff;
	text-shadow:0 0 5px #000;
}
