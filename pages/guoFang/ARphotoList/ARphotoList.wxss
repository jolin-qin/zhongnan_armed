
.container {
	min-height: 100%;
	background: url("https://zntest.wxumi.com/guofang/AR/jzhz_bj.png")no-repeat center top;
	background-size: cover;
}
.title__img {
    display: block;
    width: 722rpx;
    height: 184rpx;
    margin: 0 auto;
}
.swiper-wrapper {
		padding-top: 12rpx;
		height: 242rpx;
		background: url("https://zntest.wxumi.com/guofang/AR/jzhz_di1.png")no-repeat center center;
		background-size: cover;
}
.history_title {
    display: block;
    width: 192rpx;
    height: 34rpx;
    margin: 0 auto;
}
.swiper-wrapper .swiper-content {
    height: 180rpx;
}
.swiper-wrapper .swiper-content .swiper-item {
    width: 242rpx !important;
    height: 178rpx !important;
    /* margin-right: 12rpx !important; */
    padding-right: 12rpx !important;
    position: relative;
}
.swiper-content .swiper-img {
    display: block;
    width: 100%;
    height: 100%;
}
.img_container {
    width: 720rpx;
    margin: 30rpx auto;
		padding-top: 1px;
		padding-bottom: 20rpx;
}
.img_container .img__top {
	width: 100%;
	height: 82rpx;
	position: relative;
}
.img_container .choose__title__img {
	display: block;
	width: 224rpx;
	height: 34rpx;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}
.img_container .img__middle {
	width: 100%;
	min-height: 778rpx;
	background: url("https://zntest.wxumi.com/guofang/AR/jzhz_di2B.png")no-repeat center center;
	background-size: cover;
	overflow: hidden;
}
.img_container .img_item {
    width: 670rpx;
    height: 366rpx;
    margin: 24rpx auto;
    border-radius: 12rpx;
    overflow: hidden;
    position: relative;
}

.no__data {
	text-align: center;
	color: #999;
	margin-top: 350rpx;
}
.img_container .img__bottom {
	display: block;
	width: 100%;
	height: 46rpx;
}