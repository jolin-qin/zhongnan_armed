/* tab切换 */
.tab_wrapper {
    width: 100%;
    white-space: nowrap;
    position: absolute;
    bottom: 0;
    left: 0;
    /* padding: 20rpx 0 0; */
    z-index: 99;
    /* background: #F3F2F7; */
    box-shadow: 0 4rpx 8rpx #e6e6e6;
}

.tab_wrapper .tab_item {
    display: inline-block;
    /* width: 224rpx; */
    width: 33.3%;
    height: 72rpx;
    line-height: 72rpx;
    text-align: center;
    color: #19725C;
    font-size: 28rpx;
    background: #F3F2F7;
    position: relative;
}

.tab_wrapper .tab_item.active::after {
    display: block;
    content: '';
    width: 194rpx;
    height: 6rpx;
    position: absolute;
    bottom: 0;
    left: 15rpx;
    background: #19725C;
}

.tab_wrapper .tab_item:first-child {
    border-radius: 6rpx 0 0 6rpx;
}

.tab_wrapper .tab_item:last-child {
    border-left: none;
    border-radius: 0 6rpx 6rpx 0;
}

