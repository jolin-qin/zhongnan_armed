page {
	background: #011715 url('https://zntest.wxumi.com/armed/bg.jpg') no-repeat center top;
    background-size: 100% auto;
    overflow-x: hidden;
}
.fixed_wrapper {
    position: fixed;
    width: 100%;
    height: 380rpx;
    left: 0;
    top: 0;
    background: #011715 url('https://zntest.wxumi.com/armed/phb_bg2.png') no-repeat center top;
    background-size: 100% 380rpx;
    z-index: 5;
}
