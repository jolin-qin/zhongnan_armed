.title_pic3 {
	display: block;
	width: 712rpx;
	height: 94rpx;
	margin: 160rpx auto 60rpx;
}
.tab_wrapper {
    width: 716rpx;
    margin: 0 auto;
}
.tab_wrapper .img_item {
    flex: 1;
    height: 66rpx;
}
.zhanwei {
    width: 100%;
    height: 380rpx;
}
.return_btn {
	display: block;
	width: 164rpx;
	height: 66rpx;
	position: fixed;
	top: 72rpx;
	right: 30rpx;
}

.rank_wrapper {
    width: 716rpx;
    border: 1px solid #138F80;
    border-top: none;
    margin: 0 auto;
    margin-bottom: 18rpx;
    /* min-height: calc(100vh - 534rpx); */
    min-height: calc(100vh - 520rpx);
    color: #88e5d9;
    background-image: linear-gradient(#003231, #003030);
}
.title-top {
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    font-weight: bold;
    background: #014444;
    
    /* padding: 0 24rpx; */
}
.title-top .first {
    width: 12%;
}
.title-top .second {
    width: 40%;
}
.title-top .third {
    width: 12%;
}
.title-top .fourth {
    width: 14%;
}
.title-top .fivth {
    width: 22%;
}
.title_top_college .first {
    width: 23%;
}
.title_top_college .second {
    width: 37%;
}
.title_top_college .third {
    width: 20%;
}
.title_top_college .fourth {
    width: 20%;
}
.rank_item {
    padding: 24rpx 0;
}
.rank_item .first_pic {
    display: block;
    width: 46rpx;
    height: 56rpx;
    margin: 0 auto;
}
.ranking {
    width: 12%;
    text-align: center;
    font-size: 32rpx;
    font-weight: bold;
}
.ranking_new {
    width: 23%;
}
.ranking_userinfo {
    width: 40%;
}
.ranking_userinfo_new {
    width: 37%;
    text-align: center;
}
.user_photo {
    width: 80rpx;
    flex: 0 0 80rpx;
    height: 80rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-right: 20rpx;
}
.rank_container {
    padding-bottom: 152rpx;
    
}
.rank_item .xing_ming {
    /* text-align: center; */
    margin-bottom: 10rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.rank_item .class_grade {
    width: 180rpx;
    font-size: 24rpx;
    height: 32rpx;
    line-height: 32rpx;
    padding: 0 10rpx;
    background-color: #77D6D3;
    border-radius: 16rpx;
    color: #000;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.total_number {
    text-align: center;
    width: 12%;
}
.total_number_new {
    width: 20%;
}
.time_consuming {
    width: 14%;
    text-align: center;
}
.time_consuming_new {
    width: 20%;
}
.medal {
    width: 22%;
    text-align: center;
}
.my_ranking {
    position: fixed;
    bottom: 0;
    width: 720rpx;
    height: 153rpx;
    left: 50%;
    transform: translateX(-50%);
    background: url('https://zntest.wxumi.com/armed/phb_bg.png') no-repeat center;
    background-size: 720rpx 153rpx;
    color: #fff;
    padding-top: 0;
    padding-bottom: 30rpx;
}
