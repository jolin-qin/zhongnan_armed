page {
	background: url('https://zntest.wxumi.com/armed/bg.jpg') no-repeat center top;
  background-size: cover;
  overflow-x: hidden;
}
.container {
	padding-top: 1px;
}
.main_title01 {
	display: block;
	width: 540rpx;
	height: 166rpx;
	margin: 170rpx auto 0;
}
.middle__part {
	width: 100%;
	margin-top: 40rpx;
}
.middle__part .swiper__wrapper {
	flex: 0 0 476rpx;
    /* height: 660rpx; */
    position: relative;
    z-index: 1;
    padding: 8rpx;
    background: url("https://zntest.wxumi.com/guofang/AI/zmjzz_xuanzekuang1.png")no-repeat center center;
    background-size: 100% 100%;
}
.swiper__wrapper .swiper__content {
	width: 100%;
	height: 660rpx;
}
.middle__part .arrow__img {
    position: absolute;
    top: 50%;
    /* margin-top: 130rpx; */
	width: 212rpx;
	height: 258rpx;
}
.middle__part .left__arrow__img {
    /* margin-left: -60rpx; */
    left: -60rpx;
}
.middle__part .right__arrow__img {
    /* margin-right: -60rpx; */
    right: -60rpx;
	transform: rotate(180deg);
}
.swiper__wrapper .mask_content {
	width: 460rpx;
    height: 90rpx;
    position: absolute;
    bottom: 8rpx;
    left: 8rpx;
    background: url("https://zntest.wxumi.com/guofang/AI/zmjzz_xuanzekuang2.png")no-repeat center center;
    background-size: cover;
    z-index: 2;
}
.mask_content .species__img {
	width: 116rpx;
	height: 58rpx;
}
.mask_content .huojianjun__img {
	width: 162rpx;
	height: 58rpx;
}




.function__btns {
	width: 600rpx;
	margin: 60rpx auto 0;
}
.picture__img {
	display: block;
	width: 250rpx;
	height: 96rpx;
}
.hazong {
    display: flex;
    justify-content: center;
    margin: 40rpx 0;
}
.hazong .haha{
    margin-right: 60rpx;
}
.hazong .van-radio__label{
    color: #fff;
}
/* 弹框css */
.information_popup {
	background-color: transparent !important;
}
.infor_popup_wrapper {
	width: 714rpx;
	height: 968rpx;
	background: url('https://zntest.wxumi.com/guofang/AI/zmjzz_tanchuang.png') no-repeat center center;
	background-size: cover;
	box-sizing: border-box;
	position: relative;
}
.infor_popup_wrapper .title {
	height: 100rpx;
	padding: 0 20rpx 0 64rpx;
}
.infor_popup_wrapper .title .species__img {
	width: 116rpx;
	height: 58rpx;
}
.infor_popup_wrapper .title .huojianjun__img {
	width: 162rpx;
	height: 58rpx;
}
.infor_popup_wrapper .title .close__img {
	width: 48rpx;
	height: 48rpx;
}
.infor_popup_wrapper .jun__introduce .jun__hui {
	display: block;
	width: 246rpx;
	height: 294rpx;
	margin: 60rpx auto;
}
.infor_popup_wrapper .jun__introduce .jun__content {
	padding: 0 60rpx;
	color: #50FFE4;
	font-size: 30rpx;
}

/* 弹框end */
/* 广告弹窗 */
.adver_popup_wrapper {
	width: 720rpx;
    height: 1280rpx;
	position: relative;
}
.adver_popup_wrapper .adver_img {
    width: 100%;
    height: 100%;
}
.count_down {
    position: absolute;
    top: 10rpx;
    /* left: 50%;
    transform: translateX(-50%); */
    right: 30rpx;
    font-weight: bold;
    text-align: center;
    /* color: #fff887; */
    color: red;
}
