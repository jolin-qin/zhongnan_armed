page{
	width: 100%;
	height: 100%;
}
.container {
	color: #333;
	font-size: 30rpx;
}
view{
	box-sizing: border-box;
}
.flex_start {
	display: flex;
	justify-content: flex-start;
	align-items: center;
}

.flex_end {
	display: flex;
	justify-content: flex-end;
	align-items: center;
}

.flex_center {
	display: flex;
	justify-content: center;
	align-items: center;
}

.flex_between {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.flex_around {
	display: flex;
	justify-content: space-around;
	align-items: center;
}
.noMore {
    font-size: 24rpx;
    text-align: center;
    line-height: 100rpx;
	color: #999;
	/* margin-bottom: 30rpx; */
}
.height {
	width: 100%;
	height: 100%;
}

@keyframes scaleout {
	0% {
		transform: scale(0.9);
	}

	25% {
		transform: scale(1.0);
	}

	50% {
		transform: scale(1.1);
	}

	75% {
		transform: scale(1.0);
	}

	100% {
		transform: scale(0.9);
	}
}
